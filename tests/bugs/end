if { [isdraw result] } {
    if { [info exists 2dviewer] } {
	clear
	smallview
	donly result
	fit
	xwd $imagedir/${test_image}.gif
    }
    if { [info exists 3dviewer] } {
	vinit
	vclear
	vdisplay result
	vsetdispmode 1
	vfit
	vdump $imagedir/${test_image}.gif
    }
    if { [info exist only_xwd] } {
	xwd $imagedir/${test_image}.gif	
    }
#} else {
#   puts "Error : The command can not be build."
#}

