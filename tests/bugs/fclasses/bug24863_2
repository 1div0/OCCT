# Test for #24863: creation of variables

box b 10 10 10
set is_ok [whatis b]

set b aaaa

box b 10 10 10

set check_ok [whatis b]
if {$is_ok != $check_ok} {
    puts "Error : results of command \"whatis\" are different"
}

set check_puts [puts $b]
if {$check_puts != ""} {
    puts "Error : result of command \"puts\" is wrong"
}