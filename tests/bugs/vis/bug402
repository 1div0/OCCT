puts "========"
puts "OCC402"
puts "========"

v2dinit

set IndexMarker 1
set TypeOfDetection 0
set X 10
set Y 10
set Width 3
set Height 3
set Angle 0

for {set i 1} {${i} <= 14} {incr i} {
     OCC402 ${IndexMarker} ${TypeOfDetection} ${X} ${Y} ${Width} ${Height} ${Angle}
     incr X 10
     incr Y 10
     incr IndexMarker 1
}

puts "There are 14 markers in the viewer"
puts "Visual checking is required !!!!"

set only_screen2d 1
