#####################################
##puts "Creating of Vertexes 2D"
##puts "(visual checking for this case is desirable)"
#####################################
v2dinit

vertex v_v 0 0 0
vertex v_v1 100 100 0
v2ddisplay v_v
v2ddisplay v_v1
v2dfit

set x_coord 405
set y_coord 7
set color2d 1

checkcolor $x_coord $y_coord 1 1 0

if { $stat != 1 } {
    puts "Error : Vertexes 2D were NOT LOADED properly"
}

set only_screen2d 1

