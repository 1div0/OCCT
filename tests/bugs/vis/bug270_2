puts "========"
puts "OCC270"
puts "========"

vinit
box box 10 10 10
vaxis axis 1 1 1 7 7 7
vdisplay box
vdisplay axis
vsetdispmode 0

set x1 172
set y1 223

QAUpdateLights
checkcolor ${x1} ${y1} 1 0 0

QASetZClippingMode FRONT
set ZClippingMode [lindex [QAGetZClippingMode] 2]
if {${ZClippingMode} != "FRONT"} {
    puts "OCC270: Error (ZClippingMode != FRONT)"
}

QAUpdateLights
checkcolor ${x1} ${y1} 1 0 0
vfit

set only_screen 1
