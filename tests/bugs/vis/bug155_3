puts "========"
puts "OCC155"
puts "========"
puts ""
###################################################
##puts "Creating of pCylinder 2D"
##puts "(visual checking for this case is desirable)"
###################################################

v2dinit

pcylinder c_y 5 10
v2ddisplay c_y
v2dfit

set x_coord 338
set y_coord 75
set color2d 1

checkcolor $x_coord $y_coord 1 1 0

if { $stat != 1 } {
    puts "Error : pCylinder 2D was NOT LOADED properly into viewer"
}

set only_screen2d 1

