puts "========"
puts "OCC38"
puts "========"

set GAMMA_CORRECTION 0.5
set env(Xw_SET_GAMMA_CORRECTION) ${GAMMA_CORRECTION}

v2dinit

set Xw_SET_GAMMA_CORRECTION [expr 1. / ${GAMMA_CORRECTION}]

OCC404 0.7 0.7 0.7

set x_coord 10
set y_coord 10
set color2d 1
checkcolor $x_coord $y_coord  0.6 0.6 0.6

set only_screen2d 1

