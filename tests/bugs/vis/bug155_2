v2dinit

pcone c_o 15 0 50 360
v2ddisplay c_o
v2dfit

set x_coord 338
set y_coord 77
set color2d 1

checkcolor $x_coord $y_coord 1 1 0

if { $stat != 1 } {
    puts "Error : pCone 2D was NOT LOADED properly into viewer"
}

set only_screen2d 1

