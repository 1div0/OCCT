v2dinit

box b 1 1 1 
v2ddisplay b
box b1 10 10 10 1 1 1 
v2ddisplay b1
v2dfit

v2derase b1

set x_coord 370
set y_coord 37
set color2d 1

checkcolor $x_coord $y_coord 0 0 0

if { $stat != 1 } {
    puts "Object was not erased"
}

set only_screen2d 1


