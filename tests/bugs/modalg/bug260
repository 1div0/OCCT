#INTERFACE IGES
puts "========"
puts "OCC260"
puts "========"

pload XDE

if { [catch { igesbrep [locate_data_file OCC260.igs] a * } catch_result] } {
    puts "OCC260: Error: there is reading problem"
} else {
    regexp {([-0-9.+eE]+) Shape\(s\) listed} [tpcompound result] full ShapeNumber
    if {${ShapeNumber} > 0} {
	puts "OCC260: OK"
    } else {
	puts "OCC260: Error"
    }
}

set length 4.41928
set 2dviewer 0
