
puts "========================"
puts "  BUC60706 "
puts " OCC110 "
puts "(case 2)"
puts "========================"

restore [locate_data_file buc60706d.brep] f 
puts [checkshape f]

restore [locate_data_file buc60706e.brep] w
puts [checkshape w]
pipe t w f

restore ${imagedir}/buc60706f.brep s
puts [checkshape s]

bcut result s t
catch {exec rm ${imagedir}/buc60706i.brep}
save result ${imagedir}/buc60706i.brep

set square 865617
set 2dviewer 0
